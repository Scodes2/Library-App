<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="638.0" prefWidth="832.0" style="-fx-background-color: Burlywood;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookstore.OwnerCustomersScreenController">
   <children>
      <TableView fx:id="viewCustomers" layoutY="99.0" prefHeight="406.0" prefWidth="832.0" style="-fx-background-color: cornsilk;">
        <columns>
          <TableColumn fx:id="usernameColumn" minWidth="277.0" prefWidth="75.0" text="Username" />
          <TableColumn fx:id="passwordColumn" minWidth="277.0" prefWidth="75.0" text="Password" />
            <TableColumn fx:id="pointsColumn" minWidth="277.0" prefWidth="75.0" text="Points" />
        </columns>
      </TableView>
      <TextField fx:id="usernameInput" layoutX="50.0" layoutY="525.0" prefHeight="25.0" prefWidth="284.0" promptText="Enter Username" style="-fx-background-color: cornsilk;" />
      <TextField fx:id="passwordInput" layoutX="358.0" layoutY="525.0" prefHeight="25.0" prefWidth="284.0" promptText="Enter Password" style="-fx-background-color: cornsilk;" />
      <Button layoutX="658.0" layoutY="525.0" mnemonicParsing="false" onAction="#addCustomer" prefHeight="25.0" prefWidth="60.0" text="Add" />
      <Button layoutX="162.0" layoutY="584.0" mnemonicParsing="false" onAction="#deleteCustomer" prefHeight="25.0" prefWidth="60.0" text="Delete" />
      <Button layoutX="558.0" layoutY="584.0" mnemonicParsing="false" onAction="#backButton" prefHeight="25.0" prefWidth="60.0" text="Back" />
      <Label fx:id="label" alignment="CENTER" layoutX="311.0" layoutY="567.0" prefHeight="17.0" prefWidth="202.0" textAlignment="CENTER" textFill="#c60101" />
      <Text fill="#9e4040" layoutX="274.0" layoutY="59.0" strokeType="OUTSIDE" strokeWidth="0.0" text="List of Customers">
         <font>
            <Font name="Bookman Old Style" size="33.0" />
         </font>
      </Text>
      <ImageView fitHeight="99.0" fitWidth="107.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bookstoreLogo.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
