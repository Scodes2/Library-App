<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="638.0" prefWidth="832.0" style="-fx-background-color: Burlywood;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bookstore.OwnerStartScreenController">
   <children>
      <Button fx:id="ownerBooksScreen" layoutX="58.0" layoutY="427.0" mnemonicParsing="false" onAction="#changeToOwnerBooksScreen" prefHeight="100.0" prefWidth="204.0" text="Books" textFill="#a18411">
         <font>
            <Font name="Bookman Old Style Italic" size="33.0" />
         </font>
      </Button>
      <Button fx:id="ownerCustomersScreen" layoutX="321.0" layoutY="427.0" mnemonicParsing="false" onAction="#changeToOwnerCustomersScreen" prefHeight="100.0" prefWidth="204.0" text="Customers" textFill="#f75914">
         <font>
            <Font name="Bookman Old Style Italic" size="29.0" />
         </font>
      </Button>
      <Button fx:id="logout" layoutX="590.0" layoutY="427.0" mnemonicParsing="false" onAction="#changeToLoginScreen" prefHeight="100.0" prefWidth="204.0" text="Logout" textFill="#792525">
         <font>
            <Font name="Bookman Old Style Italic" size="33.0" />
         </font>
      </Button>
      <ImageView fitHeight="352.0" fitWidth="394.0" layoutX="240.0" layoutY="28.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bookstoreLogo.jpg" />
